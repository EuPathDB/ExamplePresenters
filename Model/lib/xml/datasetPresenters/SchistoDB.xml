  <datasetPresenter name="shaeEgypt_primary_genome_RSRC"
                    projectName="SchistoDB"
                    >
    <displayName><![CDATA[Genome Sequence and Annotation]]></displayName>
    <shortDisplayName></shortDisplayName>
    
    <summary><![CDATA[Genome Sequence and Annotation for <i>Schistosoma haematobium</i> Egypt]]></summary>
    <description><![CDATA[
      The genome sequence and annotation of <i>Schistosoma haematobium</i> Egypt was obtained from Genbank
      <br><br>

      ]]></description>
    <protocol></protocol>
    <caveat></caveat>
    <acknowledgement></acknowledgement>
    <releasePolicy></releasePolicy>
    <history buildNumber="24" 
            genomeSource="Genbank" genomeVersion="Sept 19, 2014" 
            annotationSource="Genbank" annotationVersion="Sept 19, 2014"/>
    <history buildNumber="29" 
            genomeSource="Genbank" genomeVersion="Apr 1, 2015" 
            annotationSource="Genbank" annotationVersion="Apr 1, 2015">re-annotated</history> 
    <primaryContactId>jessica.kissinger</primaryContactId>
    <contactId>guilherme.correa.de.oliveira-fiocruz</contactId>
    <contactId>francislon.silva.de.oliveira-fiocruz</contactId>
    <link>
      <text>Genbank Bioproject</text>
      <url>http://www.ncbi.nlm.nih.gov/bioproject/PRJNA78265</url>
    </link>
    <pubmedId></pubmedId>
    <templateInjector className="org.apidb.apicommon.model.datasetInjector.AnnotatedGenome">
      <prop name="isEuPathDBSite">false</prop>
      <prop name="optionalSpecies"></prop>
      <prop name="specialLinkDisplayText"></prop>
      <prop name="updatedAnnotationText"></prop>
      <prop name="isCurated">false</prop>
      <prop name="specialLinkExternalDbName"></prop>
      <prop name="showReferenceTranscriptomics">false</prop>
    </templateInjector>
  </datasetPresenter>



